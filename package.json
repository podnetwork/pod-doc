{
	"name": "pod-doc-sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "pnpm prebuild && vite dev",
		"build": "pnpm docsync && pnpm prebuild && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"docsync": "sh ./doc-sync.sh",
		"remark:test": "node ./src/lib/md/remark/custom-tag/test.js",
		"docsync:local": "sh ./doc-sync-local.sh",
		"transform-yaml-data:test": "node ./transform-yaml-data.test.js",
		"normalize-markdown:test": "node ./src/lib/md/normalize-md-preprocess/index.test.js",
		"prebuild:test": "node ./src/lib/prebuild/index.test.js",
		"prebuild": "node ./src/lib/prebuild/main.js",
		"deploy:main": "curl -X POST https://api.vercel.com/v1/integrations/deploy/prj_OmjAl7G5s7kt3X5o9zbUanmswKaj/UKKTD8zcXp",
		"deploy:dev": "curl -X POST https://api.vercel.com/v1/integrations/deploy/prj_OmjAl7G5s7kt3X5o9zbUanmswKaj/r8n5uYju4w"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.26.0",
		"@lucide/svelte": "^0.482.0",
		"@sveltejs/adapter-vercel": "^5.7.1",
		"@sveltejs/kit": "^2.20.8",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/mdast": "^4.0.4",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.4.4",
		"clsx": "^2.1.1",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "^2.0.1",
		"globals": "^15.14.0",
		"hastscript": "^9.0.1",
		"json5": "^2.2.3",
		"mdsvex": "^0.12.5",
		"mode-watcher": "^0.5.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"remark": "^15.0.1",
		"remark-abbr": "^1.4.2",
		"shiki": "^3.3.0",
		"svelte": "^5.28.2",
		"svelte-check": "^4.1.7",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.25.0",
		"tailwind-merge": "^3.2.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.31.1",
		"unist-util-visit": "^5.0.0",
		"vaul-svelte": "1.0.0-next.6",
		"vite": "^6.3.4",
		"zod": "^3.24.3"
	},
	"dependencies": {
		"@clerk/localizations": "^3.14.4",
