		"curl": "link:@shikijs/langs/curl",
		"fuse.js": "^7.1.0",
		"glob": "^11.0.2",
		"js-yaml": "^4.1.0",
		"mdast": "^3.0.0",
		"rxjs": "^7.8.2",
		"svelte-clerk": "^0.10.10",
		"tiged": "^2.12.7",
		"web3": "^4.16.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@clerk/shared",
			"esbuild"
		]
	}
}
