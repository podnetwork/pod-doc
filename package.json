{
	"name": "pod-doc-sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm docsync && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"docsync": "sh ./doc-sync.sh",
		"remark:codeimport:test": "node ./src/lib/md/remark/code-import/test.js",
		"remark:content:test": "node ./src/lib/md/remark/content/test.js"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.8",
		"@eslint/js": "^9.24.0",
		"@lucide/svelte": "^0.482.0",
		"@sveltejs/adapter-vercel": "^5.7.0",
		"@sveltejs/kit": "^2.20.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/mdast": "^4.0.4",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.3.19",
		"clsx": "^2.1.1",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^2.46.1",
		"formsnap": "^2.0.1",
		"globals": "^15.14.0",
		"hastscript": "^9.0.1",
		"json5": "^2.2.3",
		"mdsvex": "^0.12.3",
		"mode-watcher": "^0.5.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"remark": "^15.0.1",
		"remark-abbr": "^1.4.2",
		"shiki": "^3.2.2",
		"svelte": "^5.27.3",
		"svelte-check": "^4.1.6",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.24.1",
		"tailwind-merge": "^3.2.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.30.1",
		"unist-util-visit": "^5.0.0",
		"vaul-svelte": "1.0.0-next.6",
		"vite": "^6.3.2",
		"zod": "^3.24.3"
	},
	"dependencies": {
		"@clerk/localizations": "^3.13.12",
		"@shikijs/langs": "^3.2.2",
		"@shikijs/themes": "^3.2.2",
		"@supabase/supabase-js": "^2.49.4",
		"fuse.js": "^7.1.0",
		"mdast": "^3.0.0",
		"rxjs": "^7.8.2",
		"svelte-clerk": "^0.10.10",
		"tiged": "^2.12.7",
		"web3": "^4.16.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@clerk/shared"
		]
	}
}
